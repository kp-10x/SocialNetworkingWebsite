<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>Register</servlet-name>
        <servlet-class>in.sp.backend.Register</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Logout</servlet-name>
        <servlet-class>in.sp.backend.Logout</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Login</servlet-name>
        <servlet-class>in.sp.backend.Login</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>UpdateProfileDetails</servlet-name>
        <servlet-class>in.sp.backend.UpdateProfileDetails</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>UpdateProfilePic</servlet-name>
        <servlet-class>in.sp.backend.UpdateProfilePic</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>UploadStatus</servlet-name>
        <servlet-class>in.sp.backend.UploadStatus</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DeleteStatus</servlet-name>
        <servlet-class>in.sp.backend.DeleteStatus</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DeleteProfile</servlet-name>
        <servlet-class>in.sp.backend.DeleteProfile</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LikeStatus</servlet-name>
        <servlet-class>in.sp.backend.LikeStatus</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Register</servlet-name>
        <url-pattern>/Register</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Logout</servlet-name>
        <url-pattern>/logout</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Login</servlet-name>
        <url-pattern>/Login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>UpdateProfileDetails</servlet-name>
        <url-pattern>/UpdateProfileDetails</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>UpdateProfilePic</servlet-name>
        <url-pattern>/UpdateProfilePic</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>UploadStatus</servlet-name>
        <url-pattern>/UploadStatus</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DeleteStatus</servlet-name>
        <url-pattern>/DeleteStatus</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DeleteProfile</servlet-name>
        <url-pattern>/DeleteProfile</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LikeStatus</servlet-name>
        <url-pattern>/LikeStatus</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
